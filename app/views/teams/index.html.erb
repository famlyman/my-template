<p style="color: green"><%= notice %></p>

<h1>Teams</h1>

<div id="teams">
  <% @teams.each do |team| %>
    <%= render team %>
    <p>
      <%= link_to "Show this team", team %>
    </p>
  <% end %>
</div>
<% if current_player&.admin? || current_player&.captain? %>
  <%= link_to "New team", new_team_path %>
<% end %>
